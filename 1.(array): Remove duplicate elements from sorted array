    int removeDuplicates(vector<int>& nums) {
        int flag=0,i=0,j=1;
        while(j<nums.size()-1){
            while(nums[i]==nums[j]){
                if(nums[i]==nums[nums.size()-1]){
                    j=nums.size()-1;
                    break;
                }
                nums[i+1]=nums[j+1];
                j++;
            }
            i++;
        }     
for(i=0;i<nums.size()-1;i++){
    if(nums[i]>=nums[i+1]){
       
        return flag+1;
    }
    flag++;
}
        
            
        
    return nums.size()  ;  
    }
